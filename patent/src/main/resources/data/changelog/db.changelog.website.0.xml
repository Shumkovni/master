            <sqlFile encoding="UTF-8"
                     relativeToChangelogFile="true"
                     splitStatements="true"
                     stripComments="true"
                     endDelimiter="\nGO"
                     path="v.1-0/01-drop-tables.sql"/>
        </rollback>
    </changeSet>

